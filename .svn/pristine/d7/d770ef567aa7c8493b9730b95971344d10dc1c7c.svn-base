            self.$alert("服务、不动产和无形资产中，本期预缴税额【" + self.fwData['B21'] + "】必须在【0.00-" + (BQYSFWYJ * 1.5).toFixed(2) + "】范围内，请修改！");
            return;
          }
          if (self.sbInfo['SZLBDM'] == '01') { //货物
            self.calculateHwSe();
          } else if (self.sbInfo['SZLBDM'] == '02') { //服务
            self.calculateFwSe();
          } else if (self.sbInfo['SZLBDM'] == '03') { //混营
            self.calculateHwSe();
            self.calculateFwSe();
          }
          //计算本年累计
