          return;
        }
        if (self.isjk == "Y") { // 强制监控，不通过不允许提交申报表
          if (!self.isCompare()) {
            self.acceptArr.splice(2, 1);
            self.acceptArr.splice(2, 1);
          }
          if (self.acceptArr.indexOf('notaccept') !== -1) {
